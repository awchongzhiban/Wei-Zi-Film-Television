#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/lint-staged@15.2.7/node_modules/lint-staged/bin/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/lint-staged@15.2.7/node_modules/lint-staged/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/lint-staged@15.2.7/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/lint-staged@15.2.7/node_modules/lint-staged/bin/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/lint-staged@15.2.7/node_modules/lint-staged/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/lint-staged@15.2.7/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/lint-staged.js" "$@"
else
  exec node  "$basedir/../../bin/lint-staged.js" "$@"
fi
