#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/rimraf@5.0.9/node_modules/rimraf/dist/esm/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/rimraf@5.0.9/node_modules/rimraf/dist/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/rimraf@5.0.9/node_modules/rimraf/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/rimraf@5.0.9/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/rimraf@5.0.9/node_modules/rimraf/dist/esm/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/rimraf@5.0.9/node_modules/rimraf/dist/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/rimraf@5.0.9/node_modules/rimraf/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/rimraf@5.0.9/node_modules:/mnt/d/allItem/rookieJava/weizi_film_television/weizi_film_television_admin_frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../dist/esm/bin.mjs" "$@"
fi
