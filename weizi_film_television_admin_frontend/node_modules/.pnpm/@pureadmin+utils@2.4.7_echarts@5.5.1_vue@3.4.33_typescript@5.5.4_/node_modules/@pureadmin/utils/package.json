{
  "name": "@pureadmin/utils",
  "version": "2.4.7",
  "packageManager": "pnpm@8.6.10",
  "description": "pureadmin utils",
  "keywords": [
    "任意JavaScript环境",
    "@pureadmin/utils",
    "typescript",
    "hooks",
    "nuxt3",
    "vue3",
    "es6"
  ],
  "homepage": "https://github.com/pure-admin/pure-admin-utils/tree/main#readme",
  "bugs": {
    "url": "https://github.com/pure-admin/pure-admin-utils/issues"
  },
  "license": "MIT",
  "author": "xiaoxian521",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pure-admin/pure-admin-utils.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/cjs.js",
  "module": "dist/index.mjs",
  "jsdelivr": "dist/iife.global.js",
  "unpkg": "dist/iife.global.js",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs.js"
    },
    "./alias": {
      "types": "./dist/alias.d.mts",
      "import": "./dist/alias.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix && prettier --write  \"src/**\" && pnpm typecheck",
    "clean:cache": "rm -rf node_modules && rm -rf .eslintcache && pnpm store prune && pnpm install",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "start": "esno src/index.ts",
    "pub": "pub",
    "prepare": "husky install"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "echarts": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@pureadmin/release": "^1.0.0",
    "@types/node": "^18.17.19",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "echarts": "^5.4.3",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "esno": "^4.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3",
    "vitest": "^0.34.5",
    "vue": "^3.3.13"
  }
}
